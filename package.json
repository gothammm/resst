{
  "name": "ress-template",
  "version": "1.0.0",
  "description": "A react.js esbuild starter template.",
  "main": "index.ts",
  "scripts": {
    "dev": "npm run build && node server.js",
    "base:css": "xtailwindcss-cli build -o styles/tailwind.css",
    "build": "NODE_ENV=development node scripts/build.js",
    "build:dev": "esbuild src/index.tsx --define:process.env.NODE_ENV=\\\"development\\\" --bundle --outfile=dist/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
