  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "antd": "^4.9.2",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "serve-static": "^1.14.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "esbuild": "^0.8.21",
    "typescript": "^4.1.2"
  }
